{
  "name": "@nuxtus/generator",
  "version": "1.7.0",
  "description": "Package responsible for generating a Nuxt page from Directus collection.",
  "main": "dist/generator.es.js",
  "module": "dist/generator.es.js",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "rollup -c",
    "lint": "eslint --fix 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxtus/generator.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "Nuxt",
    "Directus",
    "generator"
  ],
  "author": "Craig Harman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuxtus/generator/issues"
  },
  "homepage": "https://github.com/nuxtus/generator#readme",
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@rollup/plugin-babel": "6.0.2",
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "9.0.2",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/nunjucks": "3.2.1",
    "eslint": "8.27.0",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "rollup": "2.79.1",
    "rollup-plugin-copy-assets": "2.0.3",
    "semantic-release": "19.0.5",
    "tslib": "2.4.1",
    "typescript": "4.8.4",
    "vitest": "0.25.1"
  },
  "dependencies": {
    "@directus/sdk": "^10.1.1",
    "chalk": "^5.0.1",
    "nanoid": "^4.0.0",
    "nunjucks": "^3.2.3",
    "openapi-typescript": "^6.0.0"
  },
  "release": {
    "branches": "main",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "revert",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "Changelog"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
