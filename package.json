{
  "name": "@nuxtus/generator",
  "version": "1.9.0",
  "description": "Package responsible for generating a Nuxt page from Directus collection.",
  "main": "dist/generator.es.js",
  "module": "dist/generator.es.js",
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "rollup -c --bundleConfigAsCjs",
    "lint": "eslint --fix 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxtus/generator.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "Nuxt",
    "Directus",
    "generator"
  ],
  "author": "Craig Harman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuxtus/generator/issues"
  },
  "homepage": "https://github.com/nuxtus/generator#readme",
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "11.1.6",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.6",
    "@semantic-release/release-notes-generator": "12.1.0",
    "@types/nunjucks": "3.2.6",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "eslint": "8.56.0",
    "rollup": "4.10.0",
    "rollup-plugin-copy-assets": "2.0.3",
    "semantic-release": "23.0.2",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vitest": "1.2.2"
  },
  "dependencies": {
    "@directus/sdk": "^15.0.0",
    "camelcase": "^8.0.0",
    "chalk": "^5.0.1",
    "nanoid": "^5.0.5",
    "nunjucks": "^3.2.3",
    "openapi-typescript": "^6.0.0"
  },
  "release": {
    "branches": "main",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular"
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "Changelog"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "volta": {
    "node": "20.11.1"
  }
}